
{
    "builds": [
      {
        "src": "app/package.json",
        "use": "@vercel/node",
        "config": {
          "distDir": "app/dist"
        }
      }
    ],
    "outputDirectory": "app/dist",
    "rewrites": [
        {
        "source": "/(.*)",
        "destination": "/index.html"
        }
    ]
  }
  